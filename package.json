{
	"name": "qr-gen",
	"version": "0.0.1",
	"engines": {
		"node": ">=16.9.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
